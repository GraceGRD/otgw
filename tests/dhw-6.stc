test "Manual DHW push in eco mode"
constraint pic16f1847

load boiler.stc
load thermostat.stc
Boiler.response = 0xC0000000

go 1200000

Serial.tx = "UI=27\r"
Serial.tx = "UI=28\r"
Serial.tx = "HW=0\r"

go 5000000
check R.000.... R00000000

Serial.tx = "HW=P\r"
go 7000000
check "HW: P"
check R.063.... R10631000
check R.000.... {}
mark

go 30000000
check R.000.... R00000000
